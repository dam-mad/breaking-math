{
  "name": "breaking-math",
  "version": "0.1.0",
  "description": "Breaking Math's official podcast website",
  "author": "Mikayda Mills <mills.ma12@gmail.com> & Dallin Parker <dallin.r.parker@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop -o",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "jest --silent",
    "test:update": "npm run test -- -u",
    "test:watch": "npm run test -- --watch",
    "test:debug": "npm run test -- --silent=false",
    "test:coverage": "npm run test -- --coverage"
  },
  "dependencies": {
    "gatsby": "^2.17.6",
    "gatsby-image": "^2.2.27",
    "gatsby-plugin-manifest": "^2.2.21",
    "gatsby-plugin-offline": "^3.0.14",
    "gatsby-plugin-preact": "^3.1.19",
    "gatsby-plugin-react-helmet": "^3.1.11",
    "gatsby-plugin-sentry": "^1.0.1",
    "gatsby-plugin-sharp": "^2.2.29",
    "gatsby-plugin-styled-components": "^3.1.11",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-source-filesystem": "^2.1.38",
    "gatsby-transformer-sharp": "^2.2.21",
    "preact": "^10.0.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1",
    "styled-components": "^4.4.0"
  },
  "devDependencies": {
    "@testing-library/react": "^9.3.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-preset-gatsby": "^0.2.18",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^3.0.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.10.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "prettier --write",
      "eslint --fix",
      "git add",
      "jest --bail --findRelatedTests"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "Breaking Math",
    "Podcast",
    "Gatsbyjs",
    "PWA"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dam-mad/breaking-math"
  }
}
