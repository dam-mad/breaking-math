{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "jest": true
  },
  "rules": {
    "max-len": 0,
    "comma-dangle": 0,
    "react/prop-types": 0,
    "object-curly-newline": 0,
    "implicit-arrow-linebreak": 0,
    "react/jsx-filename-extension": 0,
    "react/destructuring-assignment": 0
  },
  "globals": {
    "Sentry": true
  }
}
